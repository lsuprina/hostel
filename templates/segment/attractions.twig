{% set module = module %}

<div class="container">

    <hr class="wow fadeIn mt-5" data-wow-delay="0.4s">

    <!--Section: Attractions-->
    <section class="mb-5 pt-3 pb-3 wow fadeIn" data-wow-delay="0.2s">
    <h5 class="text-center dark-grey-text text-uppercase font-weight-bold spacing my-5 pt-2 pb-4 wow fadeIn"
        data-wow-delay="0.2s">
        <strong>{{module.titleAttractions}}</strong>
    </h5>

    <!--Grid row-->
    <div class="row">

        <!--Grid column-->
        <div class="col-lg-5 col-md-12 mb-4">

        <img class="img-fluid z-depth-1 rounded" src="{{module.image.one().url}}"
            alt="Card image cap">

        </div>
        <!--Grid column-->

        <!--Grid column-->
        <div class="col-lg-7 col-md-12">

        <div class="grey-text">{{module.description}}</div>


        <!--Grid row-->
        <div class="row mt-4">

            <!--Grid column-->
            <div class="col-md-6 col-6 mb-4">
                <div class="col-1 col-md-2 float-left">
                    <i class="fas fa-camera-retro fa-2x info-text"></i>
                </div>
                <div class="col-10 col-md-9 col-lg-10 float-right">
                    <div class="font-weight-bold info-text mt-2">{{module.leftHeading}}</div>
                    <div class="grey-text">{{module.leftParagraph}}</div>
                </div>
            </div>
            <!--Grid column-->

            <!--Grid column-->
            <div class="col-md-6 col-6 mb-4">
                <div class="col-1 col-md-2 float-left">
                    <i class="fas fa-tint fa-2x info-text"></i>
                </div>
                <div class="col-10 col-md-9 col-lg-10 float-right">
                    <div class="font-weight-bold info-text mt-2 ">{{module.rightHeading}}</div>
                    <div class="grey-text">{{module.rightParagraph}}</div>
                </div>
            </div>
            <!--Grid column-->


        </div>
        <!--Grid row-->

        </div>
        <!--Grid column-->

    </div>
    <!--Grid row-->

    </section>
    <!--/Section: Attractions-->
</div>